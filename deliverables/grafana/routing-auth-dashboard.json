{
  "__inputs": [],
  "__requires": [
    {
      "type": "datasource",
      "id": "grafana-prometheus-datasource",
      "name": "Prometheus",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    }
  ],
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "panels": [
    {
      "title": "401 responses (5m rate)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(increase(routing_ml_auth_401_total[5m]))",
          "interval": "",
          "legendFormat": "All endpoints",
          "refId": "A"
        },
        {
          "expr": "sum(increase(routing_ml_auth_401_total{endpoint=\"/api/predict\"}[5m]))",
          "legendFormat": "/api/predict",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 6 },
              { "color": "red", "value": 15 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Auth handshake p95 latency",
      "type": "stat",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le)(rate(routing_ml_auth_handshake_seconds_bucket[5m])))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 0.8 },
              { "color": "red", "value": 1.5 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "401 by endpoint (last 30m)",
      "type": "stat",
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 8 },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "vertical",
        "textMode": "value_and_name"
      },
      "targets": [
        {
          "expr": "sum(increase(routing_ml_auth_401_total[30m])) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "title": "Handshake latency histogram",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 14 },
      "targets": [
        {
          "expr": "rate(routing_ml_auth_handshake_seconds_bucket[5m])",
          "legendFormat": "{{le}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["routing-ml", "auth"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Routing-ML Auth Overview",
  "uid": "routing-auth",
  "version": 1
}
